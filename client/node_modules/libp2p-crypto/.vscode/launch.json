{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node2",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/aegir-test",
      "stopOnEntry": false,
      "args": ["--env", "node"],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": "/Users/dignifiedquire/.nvm/versions/node/v6.6.0/bin/node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "testing"
      },
      "console": "internalConsole",
      "sourceMaps": false,
      "outFiles": []
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": false,
      "sourceMaps": false,
      "outFiles": [],
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command.PickProcess}",
      "port": 5858,
      "sourceMaps": false,
      "outFiles": []
    }
  ]
}